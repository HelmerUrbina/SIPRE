	<property name="ireport.zoom" value="5.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="PERIODO" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["2018"]]></defaultValueExpression>
	</parameter>
	<parameter name="MES" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["03"]]></defaultValueExpression>
	</parameter>
	<parameter name="CODIGO" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["44469132"]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
PM.NIMPORTE_MOVILIDAD,
(SELECT COUNT(*) FROM SIPE_PLANILLA_MOVILIDAD MOV WHERE MOV.CPERIODO_CODIGO=PM.CPERIODO_CODIGO AND
MOV.CMES_CODIGO=PM.CMES_CODIGO AND MOV.CESTADO_CODIGO='CE' AND MOV.NIMPORTE_MOVILIDAD=PM.NIMPORTE_MOVILIDAD) AS CANTIDAD
FROM SIPE_PLANILLA_MOVILIDAD PM
WHERE PM.CPERIODO_CODIGO=$P{PERIODO} AND PM.CMES_CODIGO=$P{MES} AND
VUSUARIO_PLANILLA=$P{CODIGO} AND
PM.CESTADO_CODIGO='CE'
GROUP BY PM.VUSUARIO_PLANILLA,PM.NIMPORTE_MOVILIDAD,
PM.CPERIODO_CODIGO,PM.CMES_CODIGO
ORDER BY PM.NIMPORTE_MOVILIDAD]]>
	</queryString>
