	<queryString>
		<![CDATA[SELECT
ABRMES,(SELECT NVL(SUM(RV.NCANTIDAD_DIAS_SOL),0) FROM SIPE_REGISTRO_VACACIONES RV WHERE RV.CPERIODO_CODIGO=$P{PERIODO} AND RV.VCODIGO_PERSONAL=$P{CODIGO} AND RV.CESTADO_REGISTRO!='AN'
AND TABMES.MESPER=TO_CHAR(RV.DUSUARIO_FECCRE,'MM')) AS CANTIDAD
FROM TABMES
GROUP BY ABRMES,MESPER
ORDER BY MESPER]]>
	</queryString>
