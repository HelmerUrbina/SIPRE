	<queryString>
		<![CDATA[SELECT UTIL_NEW.FUN_NOMBRE_GRADO(RP.CCODIGO_GRADO) AS GRADO,
RP.VDNI_PERSONAL AS DNI,
UTIL_NEW.FUN_NOMBRE_PERSONAL(RP.VDNI_PERSONAL) AS PERSONAL,
LOWER(UTIL.NUMERO_A_LETRAS(RV.NCANTIDAD_DIAS_SOL))||' ('||LPAD(RV.NCANTIDAD_DIAS_SOL,2,0)||')' AS DIAS,
INITCAP(to_char(DFECHA_INICIO, 'DAY', 'NLS_DATE_LANGUAGE=SPANISH'))||' '||TO_CHAR(DFECHA_INICIO,'DD')||' de '||
INITCAP(UTIL_NEW.FUN_NONMES(TO_CHAR(DFECHA_INICIO,'MM')))||' '||TO_CHAR(DFECHA_INICIO,'YYYY') AS EMPIEZA,
INITCAP(to_char(DFECHA_FIN, 'DAY', 'NLS_DATE_LANGUAGE=SPANISH'))||' '||TO_CHAR(DFECHA_FIN,'DD')||' de '||
INITCAP(UTIL_NEW.FUN_NONMES(TO_CHAR(DFECHA_FIN,'MM')))||' '||TO_CHAR(DFECHA_FIN,'YYYY') AS TERMINA,
INITCAP(RP.VDIRECCION_PERSONAL) AS DIRECCION,
RP.VNUMERO_TELEFONO AS TELEFONO,
LPAD(RV.NCANTIDAD_DIAS_SOL,2,0) AS HACE_USO,
LPAD(RV.NCANTIDAD_DIAS_REST,2,0) AS QUEDAN,
LPAD((30-(RV.NCANTIDAD_DIAS_REST+RV.NCANTIDAD_DIAS_SOL)),2,0) AS HIZO_USO,
TO_CHAR(RV.DUSUARIO_FECCRE,'DD')||' de '||INITCAP(UTIL_NEW.FUN_NONMES(TO_CHAR(RV.DUSUARIO_FECCRE,'MM')))||' del '||TO_CHAR(RV.DUSUARIO_FECCRE,'YYYY') AS FECHA,
RV.VMOTIVO_VACACIONES AS MOTIVO
FROM SIPE_REGISTRO_VACACIONES RV JOIN SIPE_REGISTRO_PERSONAL RP ON
(RV.VCODIGO_PERSONAL=RP.VDNI_PERSONAL)
WHERE
RV.CPERIODO_CODIGO=$P{PERIODO} AND
RV.NCORRELATIVO=$P{CODIGO} AND
RV.VCODIGO_PERSONAL=$P{USUARIO} AND
CESTADO_REGISTRO!='AN'
UNION ALL
SELECT UTIL_NEW.FUN_NOMBRE_GRADO(RP.CCODIGO_GRADO) AS GRADO,
RP.VDNI_PERSONAL AS DNI,
UTIL_NEW.FUN_NOMBRE_PERSONAL(RP.VDNI_PERSONAL) AS PERSONAL,
LOWER(UTIL.NUMERO_A_LETRAS(RV.NCANTIDAD_DIAS_SOL))||' ('||LPAD(RV.NCANTIDAD_DIAS_SOL,2,0)||')' AS DIAS,
INITCAP(to_char(DFECHA_INICIO, 'DAY', 'NLS_DATE_LANGUAGE=SPANISH'))||' '||TO_CHAR(DFECHA_INICIO,'DD')||' de '||
INITCAP(UTIL_NEW.FUN_NONMES(TO_CHAR(DFECHA_INICIO,'MM')))||' '||TO_CHAR(DFECHA_INICIO,'YYYY') AS EMPIEZA,
INITCAP(to_char(DFECHA_FIN, 'DAY', 'NLS_DATE_LANGUAGE=SPANISH'))||' '||TO_CHAR(DFECHA_FIN,'DD')||' de '||
INITCAP(UTIL_NEW.FUN_NONMES(TO_CHAR(DFECHA_FIN,'MM')))||' '||TO_CHAR(DFECHA_FIN,'YYYY') AS TERMINA,
INITCAP(RP.VDIRECCION_PERSONAL) AS DIRECCION,
RP.VNUMERO_TELEFONO AS TELEFONO,
LPAD(RV.NCANTIDAD_DIAS_SOL,2,0) AS HACE_USO,
LPAD(RV.NCANTIDAD_DIAS_REST,2,0) AS QUEDAN,
LPAD((30-(RV.NCANTIDAD_DIAS_REST+RV.NCANTIDAD_DIAS_SOL)),2,0) AS HIZO_USO,
TO_CHAR(RV.DUSUARIO_FECCRE,'DD')||' de '||INITCAP(UTIL_NEW.FUN_NONMES(TO_CHAR(RV.DUSUARIO_FECCRE,'MM')))||' del '||TO_CHAR(RV.DUSUARIO_FECCRE,'YYYY') AS FECHA,
RV.VMOTIVO_VACACIONES AS MOTIVO
FROM SIPE_REGISTRO_VACACIONES RV JOIN SIPE_REGISTRO_PERSONAL RP ON
(RV.VCODIGO_PERSONAL=RP.VDNI_PERSONAL)
WHERE
RV.CPERIODO_CODIGO=$P{PERIODO} AND
RV.NCORRELATIVO=$P{CODIGO} AND
RV.VCODIGO_PERSONAL=$P{USUARIO} AND
CESTADO_REGISTRO!='AN']]>
	</queryString>
