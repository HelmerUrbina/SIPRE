	<parameter name="USUARIO" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["44469132"]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT PM.VUSUARIO_PLANILLA AS CODIGO,
UTIL_NEW.FUN_NONMES(PM.CMES_CODIGO) AS MES,
PM.CPERIODO_CODIGO AS PERIODO,
UTIL_NEW.FUN_NOMUSU('0003',PM.VUSUARIO_PLANILLA) AS NOMBRE,
UTIL_NEW.FUN_TOTAL_PLANILLA_MOVILIDAD(PM.CPERIODO_CODIGO,PM.CMES_CODIGO,PM.VUSUARIO_PLANILLA) AS SUB_TOTAL,
CASE WHEN $P{USUARIO} IN ('10714635','43305891','44469132') THEN 'Sección Telemática - OPRE' ELSE
UTIL_NEW.FUN_DESC_USUARIO($P{USUARIO}) END AS USU_IMPRESION
FROM SIPE_PLANILLA_MOVILIDAD PM
WHERE PM.CPERIODO_CODIGO=$P{PERIODO} AND PM.CMES_CODIGO=$P{MES} AND
PM.CESTADO_CODIGO='CE'
GROUP BY PM.VUSUARIO_PLANILLA,
PM.CPERIODO_CODIGO,PM.CMES_CODIGO
ORDER BY NOMBRE]]>
	</queryString>
