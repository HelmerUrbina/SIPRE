	<queryString>
		<![CDATA[SELECT CPERIODO_CODIGO AS PERIODO,
VUSUARIO_PLANILLA AS COD_USU,
TO_CHAR(LAST_DAY(TO_DATE('01'||'/'||CMES_CODIGO||'/'||CPERIODO_CODIGO,'DD/MM/YYYY')),'DD') AS ULT_DIA,
UTIL_NEW.FUN_NOMUSU('0003',VUSUARIO_PLANILLA) AS USUARIO,
UTIL.NUMERO_A_LETRAS(UTIL_NEW.FUN_TOTAL_PLANILLA_MOVILIDAD(CPERIODO_CODIGO,CMES_CODIGO,VUSUARIO_PLANILLA)) AS NUM_LETRA,
initcap(UTIL_NEW.FUN_NONMES(CMES_CODIGO)) AS MES,
TO_CHAR(DFECHA_MOVILIDAD,'DD/MM/YYYY') AS FECHA_MOVILIDAD,
UTIL_NEW.FUN_NOMBRE_UBIGEO(CLUGAR_ORIGEN) AS ORIGEN,
CASE CLUGAR_DESTINO WHEN '150101' THEN 'CERCADO DE LIMA' ELSE
UTIL_NEW.FUN_NOMBRE_UBIGEO(CLUGAR_DESTINO) END AS DESTINO,
SUM(NIMPORTE_MOVILIDAD) AS IMPORTE,
UTIL_NEW.FUN_TOTAL_PLANILLA_MOVILIDAD(CPERIODO_CODIGO,CMES_CODIGO,VUSUARIO_PLANILLA) AS TOTAL
FROM SIPE_PLANILLA_MOVILIDAD
WHERE CPERIODO_CODIGO=$P{PERIODO} AND CMES_CODIGO=$P{TIPO} AND
VUSUARIO_PLANILLA=$P{CODIGO}
GROUP BY DFECHA_MOVILIDAD,CLUGAR_ORIGEN,CLUGAR_DESTINO,
VUSUARIO_PLANILLA,CPERIODO_CODIGO,CMES_CODIGO
ORDER BY FECHA_MOVILIDAD]]>
	</queryString>
