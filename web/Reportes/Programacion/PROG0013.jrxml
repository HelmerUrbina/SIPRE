	<queryString>
		<![CDATA[SELECT 
CODPER AS PERIODO, 
COUUOO||':'||UTIL_NEW.FUN_NOUUOO(COUUOO) AS UNIDAD_OPERATIVA, 
UTIL_NEW.FUN_DEPPTO(COPPTO) AS PRESUPUESTO, 
COMEOP||'-'||UTIL_NEW.FUN_NTAREA(COMEOP) AS TAREA,
CODEVE||':'||UTIL_NEW.FUN_NOMEVP(CODEVE, COMEOP, CODDEP, COUUOO, CODPER, COPPTO) AS EVENTO,
COCAGA||':'||UTIL_NEW.FUN_NOCLAS(COCAGA) AS CADENA_GASTO, 
DESITM AS ITEM, 
CANREQ*PREVTA AS TOTAL,
CASE WHEN $P{USUARIO} IN ('10714635','43305891','44469132') THEN 'Sección Telemática - OPRE' ELSE 
UTIL_NEW.FUN_DESC_USUARIO($P{USUARIO}) END AS USU_IMPRESION 
FROM TAHOTR WHERE 
CODPER=$P{PERIODO} AND
COPPTO=$P{PRESUPUESTO} AND
COUUOO=$P{UNIDAD_OPERATIVA} AND
COMEOP=$P{CODIGO2} AND
CODEVE LIKE '%'||SUBSTR($P{CODIGO},4,LENGTH($P{CODIGO}))||'%' 
AND ESTREG!='AN'
ORDER BY CODEVE, CADENA_GASTO, ITEM]]>
	</queryString>
