		<defaultValueExpression><![CDATA["2017"]]></defaultValueExpression>
	</parameter>
	<parameter name="PRESUPUESTO" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[1]]></defaultValueExpression>
	</parameter>
	<parameter name="USUARIO" class="java.lang.String">
		<defaultValueExpression><![CDATA["43305891"]]></defaultValueExpression>
	</parameter>
	<parameter name="CODIGO" class="java.lang.String">
		<defaultValueExpression><![CDATA["11"]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT CPERIODO_CODIGO AS PERIODO,
UTIL_NEW.FUN_DEPPTO(NPRESUPUESTO_CODIGO) AS FUENTE_FINANCIAMIENTO,
NPRIORIZACION_CODIGO AS CODIGO,
VCADENA_GASTO_CODIGO||':'||UTIL_NEW.FUN_NOMBRE_CADENA_GASTO(VCADENA_GASTO_CODIGO) AS CADENA_GASTO,
CASE CPRIORIZACION_TIPO WHEN 'I' THEN SUM(NPRIORIZACION_IMPORTE) ELSE 0.0 END AS INCREMENTO,
CASE CPRIORIZACION_TIPO WHEN 'D' THEN SUM(NPRIORIZACION_IMPORTE)*(-1) ELSE 0.0 END AS DECREMENTO,
CASE WHEN $P{USUARIO} IN ('10714635','43305891','44469132') THEN 'Sección Telemática - OPRE' ELSE 
UTIL_NEW.FUN_DESC_USUARIO($P{USUARIO}) END AS USU_IMPRESION 
FROM SIPRE_PRIORIZACION_DETALLE WHERE 
CPERIODO_CODIGO=$P{PERIODO} AND 
NPRESUPUESTO_CODIGO=$P{PRESUPUESTO} AND 
NPRIORIZACION_CODIGO=$P{CODIGO}
GROUP BY CPERIODO_CODIGO, NPRESUPUESTO_CODIGO, NPRIORIZACION_CODIGO, CPRIORIZACION_TIPO, VCADENA_GASTO_CODIGO
ORDER BY CADENA_GASTO]]>
	</queryString>
