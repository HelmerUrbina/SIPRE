	<queryString>
		<![CDATA[SELECT  TACESO.NROCOB,UTIL_NEW.FUN_TIPO_CERTIFICADO(TACESO.CODPER,TACESO.COPPTO,TACESO.NROCOB) TIPO,
CASE UTIL_NEW.FUN_TIPO_CERTIFICADO(TACESO.CODPER,TACESO.COPPTO,TACESO.NROCOB)WHEN 'CE' THEN 'CERTIFICADO'
WHEN 'RE' THEN 'REBAJA' WHEN 'AM' THEN 'AMPLIACION' END TIPO1,
CASE WHEN TACESO.NCSIAF IS NULL THEN ' ' ELSE 'REGISTRO SIAF: '||TACESO.NCSIAF END SIAF,
(SELECT CASE WHEN TRIM(DIPPTO)='S' THEN (case when taceso.estccp = 'AN' then 'ANULADO' ELSE ' ' END) ELSE 'COPIA' END FROM TABUSU WHERE  CODUSU=$P{USUARIO} AND ESTREG='AC')  ESTADO,
CASE WHEN TACESO.NCSIAF IS NULL THEN 'FALTA REGISTRO SIAF' ELSE '' END AS ESTADO_SIAF,
taceso.nusoce,
taceso.numsol,
OPRE_NEW.FUN_MONTO_CCP(TACESO.CODPER, TACESO.COPPTO, TACESO.MESPER, DECESO.CODGEN , taceso.nusoce ) MOCRPR,
TACESO.NUMOFI,
to_char(taceso.fecccp,'DD')||' de '||util.fun_nonmes(to_char(taceso.fecccp,'MM'))||' '||TACESO.CODPER fecccp,
TACESO.CODPER PERIODO,
Util.fun_deppto(TACESO.coppto) FUENTE,
DECESO.CODGEN GENERICA,
CASE WHEN TRIM(DECESO.CODGEN) = '1'  THEN 'X' ELSE ' ' END GENERICA1, 
CASE WHEN TRIM(DECESO.CODGEN) = '3'  THEN 'X' ELSE ' ' END GENERICA3, 
CASE WHEN TRIM(DECESO.CODGEN) = '5'  THEN 'X' ELSE ' ' END GENERICA5, 
CASE WHEN TRIM(DECESO.CODGEN) = '6'  THEN 'X' ELSE ' ' END GENERICA6, 
CASE WHEN TRIM(DECESO.CODGEN) = '1'  
THEN OPRE.FUN_MONCOB_CADFUNPRG(TACESO.CODPER, TACESO.coppto, TACESO.NROCOB, DECESO.CODFUN,DECESO.CODPRG,DECESO.COSPRG,
DECESO.CODACT,DECESO.CODCOM,DECESO.CODMET, 
TACESO.mesper, taceso.nusoce, 'S',DECESO.CODGEN) ELSE NULL END MONTO_G1_S, 
CASE WHEN TRIM(DECESO.CODGEN) = '3'
  THEN OPRE.FUN_MONCOB_CADFUNPRG(TACESO.CODPER, TACESO.coppto, TACESO.NROCOB, DECESO.CODFUN,DECESO.CODPRG,DECESO.COSPRG,
                          DECESO.CODACT,DECESO.CODCOM,DECESO.CODMET, 
TACESO.mesper, taceso.nusoce,'S',DECESO.CODGEN) ELSE NULL END MONTO_G3_S, 
CASE WHEN TRIM(DECESO.CODGEN) = '5'  THEN OPRE.FUN_MONCOB_CADFUNPRG(TACESO.CODPER, TACESO.coppto, TACESO.NROCOB, DECESO.CODFUN,DECESO.CODPRG,DECESO.COSPRG,
                          DECESO.CODACT,DECESO.CODCOM,DECESO.CODMET,
 TACESO.mesper, taceso.nusoce,'S',DECESO.CODGEN) ELSE NULL END MONTO_G5_S, 
CASE WHEN TRIM(DECESO.CODGEN) = '6'  THEN OPRE.FUN_MONCOB_CADFUNPRG(TACESO.CODPER, TACESO.coppto, TACESO.NROCOB, DECESO.CODFUN,DECESO.CODPRG,DECESO.COSPRG,
                          DECESO.CODACT,DECESO.CODCOM,
DECESO.CODMET, TACESO.mesper, taceso.nusoce,'S',DECESO.CODGEN) ELSE NULL END MONTO_G6_S, 
DECESO.COCAGA ||'    '||  UTIL.FUN_NOMBRE_CADGAS(DECESO.COCAGA) CADENA_GASTO,
Util.fun_nonmes(TACESO.mesper) MES,
TACESO.couuoo ||':'||Util.fun_nouuoo(TACESO.couuoo) UNIDADCARGO,
DECESO.CODMET,
TACAFU.SECFUN||'.'||UTIL_NEW.FUN_DESMET_TACAFU(TACESO.CODPER, TACESO.coppto, DECESO.SECFUN) CADENA_FUNCIONAL,
taceso.docref GLOSA,
SUM(import) importe,
(select tabprs.codprs from tabprs,tapsre where tapsre.codper=tabprs.codper and tapsre.codprs=tabprs.codprs and tapsre.capsre='1' and tapsre.codrpt='046' and tabprs.codper = $P{PERIODO}) as firma1,
(select tabprs.apeprs from tabprs,tapsre where tapsre.codper=tabprs.codper and tapsre.codprs=tabprs.codprs and tapsre.capsre='1' and tapsre.codrpt='046' and tabprs.codper = $P{PERIODO}) as firma1a,
(select tabprs.nomprs from tabprs,tapsre where tapsre.codper=tabprs.codper and tapsre.codprs=tabprs.codprs and tapsre.capsre='1' and tapsre.codrpt='046' and tabprs.codper = $P{PERIODO}) as firma1b,
(select tabprs.graprs from tabprs,tapsre where tapsre.codper=tabprs.codper and tapsre.codprs=tabprs.codprs and tapsre.capsre='1' and tapsre.codrpt='046' and tabprs.codper = $P{PERIODO}) as firma1c,
(select tabprs.carprs from tabprs,tapsre where tapsre.codper=tabprs.codper and tapsre.codprs=tabprs.codprs and tapsre.capsre='1' and tapsre.codrpt='046' and tabprs.codper = $P{PERIODO}) as firma1d,
(select tabprs.codprs from tabprs,tapsre where tapsre.codprs=tabprs.codprs AND TAPSRE.CODPER=TABPRS.CODPER and tapsre.capsre='2' and tapsre.codrpt='046' AND TABPRS.CODPER=$P{PERIODO}) AS firma2, 
(select tabprs.apeprs from tabprs,tapsre where tapsre.codprs=tabprs.codprs AND TAPSRE.CODPER=TABPRS.CODPER and tapsre.capsre='2' and tapsre.codrpt='046' AND TABPRS.CODPER=$P{PERIODO}) AS firma2a,
(select tabprs.nomprs from tabprs,tapsre where tapsre.codprs=tabprs.codprs AND TAPSRE.CODPER=TABPRS.CODPER and tapsre.capsre='2' and tapsre.codrpt='046' AND TABPRS.CODPER=$P{PERIODO}) AS firma2b,
(select tabprs.graprs from tabprs,tapsre where tapsre.codprs=tabprs.codprs AND TAPSRE.CODPER=TABPRS.CODPER and tapsre.capsre='2' and tapsre.codrpt='046' AND TABPRS.CODPER=$P{PERIODO}) AS firma2c,
(select tabprs.carprs from tabprs,tapsre where tapsre.codprs=tabprs.codprs AND TAPSRE.CODPER=TABPRS.CODPER and tapsre.capsre='2' and tapsre.codrpt='046' AND TABPRS.CODPER=$P{PERIODO}) AS firma2d,
CASE WHEN $P{USUARIO} IN ('10714635','43305891','44469132') THEN 'Sección Telemática - OPRE' ELSE 
UTIL_NEW.FUN_DESC_USUARIO($P{USUARIO}) END AS USU_IMPRESION 
FROM DECESO, TACESO, TACAFU 
WHERE 
TACESO.codper =  $P{PERIODO} AND
TACESO.coppto = $P{PRESUPUESTO} AND
TACESO.NROCOB = $P{CODIGO} AND
DECESO.CODPER = TACAFU.CODPER and
DECESO.COPPTO = TACAFU.COPPTO and
DECESO.CODFUN = TACAFU.CODFUN and
DECESO.CODPRG = TACAFU.CODPRG and
DECESO.COSPRG = TACAFU.COSPRG and
DECESO.CODACT = TACAFU.CODACT and
DECESO.CODCOM = TACAFU.CODCOM and
DECESO.CODMET = TACAFU.CODMET and 
DECESO.SECFUN=TACAFU.SECFUN AND 
TACESO.CODPER = DECESO.CODPER and
TACESO.COPPTO = DECESO.COPPTO AND
--TACESO.MESPER = DECESO.MESPER AND
TACESO.NROCOB = DECESO.NROCOB AND
TACESO.NUSOCE = DECESO.NUSOCE AND
TACESO.NUMSOL = DECESO.NUMSOL
GROUP BY 
TACESO.CODPER, TACESO.coppto, TACESO.mesper, TACESO.couuoo, TACESO.NROCOB,taceso.estccp,
DECESO.CODFUN,DECESO.CODPRG,DECESO.COSPRG,DECESO.COPRES ,DECESO.CODACT,DECESO.CODCOM,DECESO.CODMET, DECESO.codgen,TACAFU.SECFUN, 
taceso.nusoce , taceso.fecccp, taceso.docref,TACESO.MOCRPR,taceso.numsol, taceso.nusoce, taceso.numsol,DECESO.SECFUN,
DECESO.COCAGA, TACESO.NUMOFI,TACESO.NCSIAF,TACESO.CODUSU
ORDER BY  TACESO.NUSOCE,deceso.codgen,
DECESO.SECFUN,
CADENA_GASTO]]>
	</queryString>
