	<queryString>
		<![CDATA[SELECT 
CODPER PERIODO, 
COPPTO||':'||UTIL_NEW.FUN_DEPPTO(COPPTO) FUENTE_FINANCIAMIENTO, TIPO,
COUUOO||':'||UTIL_NEW.FUN_ABUUOO(COUUOO) UNIDAD_OPERATIVA, 
FEC_REGISTRO,
UTIL_NEW.FUN_NOMUSU('0003', CODUSU) SECTORISTA,
GLOSA, 
IMPORTE, 
CASE WHEN $P{USUARIO} IN ('10714635','43305891','44469132') THEN 'Sección Telemática - OPRE' ELSE 
UTIL_NEW.FUN_DESC_USUARIO($P{USUARIO}) END AS USU_IMPRESION 
FROM V_AVANCE_EJECUCION_DIARIO WHERE 
CODPER=$P{PERIODO} AND
CODUSU=$P{USUARIO} AND
FEC_REGISTRO LIKE '%'||$P{CODIGO}||'%'
ORDER BY SECTORISTA, TIPO, GLOSA]]>
