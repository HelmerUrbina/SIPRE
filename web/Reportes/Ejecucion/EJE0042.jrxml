	<queryString>
		<![CDATA[SELECT DE.CODPER,DE.NUMPED,DE.COPPTO,UTIL.FUN_DEPPTO(DE.COPPTO) DEPPTO,
CASE WHEN DE.COTIDE='007' AND TIPO='J' THEN
UTIL.FUN_DES_BENEFICIO_RESOLUCIONES(DE.TIPO,DE.COTIDE)||' A FAVOR DEL '||UTIL.FUN_DES_GRADO_JADPE(DE.PERIODO_RESOL,DE.NUMRES,DE.NUMOFI,CODBEN,DE.COTIDE)||' '||
UTIL.FUN_DES_TITULAR_JADPE(DE.PERIODO_RESOL,DE.NUMRES,DE.NUMOFI,CODBEN,DE.COTIDE)||' (CAUSANTE: '|| UTIL.FUN_NOM_CAUSANTE_JADPE(DE.PERIODO_RESOL,DE.NUMRES,DE.NUMOFI,CODBEN,DE.COTIDE)||') RESOLUCION '||DE.NUMRES
||' ('||TO_CHAR((SELECT DISTINCT FECRES FROM RESODP WHERE CODPER=DE.CODPER AND COPPTO=DE.COPPTO AND NUMRES=DE.NUMRES AND NUMOFI=DE.NUMOFI),'DD/MM/YYYY')||') OFICIO '||DE.NUMOFI||' ('||
TO_CHAR((SELECT DISTINCT FECOFI FROM RESODP WHERE CODPER=DE.CODPER AND COPPTO=DE.COPPTO AND NUMRES=DE.NUMRES AND NUMOFI=DE.NUMOFI),'DD/MM/YYYY')||').'
ELSE 
UTIL.FUN_DES_BENEFICIO_RESOLUCIONES(DE.TIPO,DE.COTIDE)||' A FAVOR DE '||UTIL_NEW.FUN_NOMBRE_GRADO(CODGRD)||' '||DE.NOMBEN||'. RESOLUCION '||DE.NUMRES||' ('||
TO_CHAR((SELECT DISTINCT FECRES FROM RESODP WHERE CODPER=DE.CODPER AND COPPTO=DE.COPPTO AND NUMRES=DE.NUMRES AND NUMOFI=DE.NUMOFI),'DD/MM/YYYY')||') OFICIO '||DE.NUMOFI||'('||
TO_CHAR((SELECT DISTINCT FECOFI FROM RESODP WHERE CODPER=DE.CODPER AND COPPTO=DE.COPPTO AND NUMRES=DE.NUMRES AND NUMOFI=DE.NUMOFI),'DD/MM/YYYY')||')' END  AS GLOSA,
SUM(IMPBEN) AS IMPORTE,
CASE WHEN $P{USUARIO} IN ('10714635','43305891','44469132') THEN 'Sección Telemática - OPRE' ELSE
UTIL_NEW.FUN_DESC_USUARIO($P{USUARIO}) END AS USU_IMPRESION
FROM DERPER DE WHERE
DE.CODPER=$P{PERIODO} AND 
DE.COPPTO=$P{PRESUPUESTO} AND
DE.NUMPED=$P{CODIGO}
GROUP BY DE.CODPER,DE.NUMPED,DE.COPPTO,DE.COTIDE,DE.NUMRES,DE.NUMOFI,CODBEN,
DE.NOMBEN,CODGRD,DE.PERIODO_RESOL,DE.TIPO
ORDER BY DE.NOMBEN]]>
	</queryString>
