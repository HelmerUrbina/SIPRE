	<queryString>
		<![CDATA[SELECT CPERIODO_CODIGO PERIODO,
NPRESUPUESTO_CODIGO||'-'||UTIL_NEW.FUN_DEPPTO(NPRESUPUESTO_CODIGO) PRESUPUESTO,
CUNIDAD_OPERATIVA_CODIGO||'-'||UTIL_NEW.FUN_ABUUOO(CUNIDAD_OPERATIVA_CODIGO) UNIDAD_OPERATIVA,
UTIL.FUN_DESCRIPCION_RESOLUCION(CPERIODO_CODIGO, NRESOLUCION_CODIGO) AS RESOLUCION,
CSECUENCIA_FUNCIONAL_CODIGO||'-'||UTIL_NEW.FUN_DESMET(CPERIODO_CODIGO, NPRESUPUESTO_CODIGO, CSECUENCIA_FUNCIONAL_CODIGO) SECUENCIA_FUNCIONAL,
CTAREA_CODIGO||'-'||UTIL_NEW.FUN_NOMEOP(TRIM(CTAREA_CODIGO)) TAREA,
VCADENA_GASTO_CODIGO||'-'||UTIL.FUN_NOMBRE_CADGAS(VCADENA_GASTO_CODIGO) CADENA_GASTO,
NPCA_AUTORIZACION AS PCA_AUTORIZACION,
SUM(NPCA_INICIAL) AS PCA_INICIAL, SUM(NPCA_IMPORTE) AS PCA, SUM(NPCA_MODIFICADO) AS PCA_MODIFICADA,
VPCA_DOCUMENTO AS JUSTIFICACION,
CASE WHEN $P{USUARIO} IN ('10714635','43305891','44469132') THEN 'Sección Telemática - OPRE' ELSE
UTIL_NEW.FUN_DESC_USUARIO($P{USUARIO}) END AS USU_IMPRESION
FROM SIPRE_PCA WHERE
CPERIODO_CODIGO=$P{PERIODO} AND
NPRESUPUESTO_CODIGO=$P{PRESUPUESTO} AND
CUNIDAD_OPERATIVA_CODIGO LIKE $P{UNIDAD_OPERATIVA} AND
NPCA_AUTORIZACION=$P{CODIGO} AND NPCA_AUTORIZACION IS NOT NULL
GROUP BY CPERIODO_CODIGO, NPRESUPUESTO_CODIGO, CUNIDAD_OPERATIVA_CODIGO, NRESOLUCION_CODIGO, CSECUENCIA_FUNCIONAL_CODIGO, CTAREA_CODIGO, VCADENA_GASTO_CODIGO, NPCA_AUTORIZACION, VPCA_DOCUMENTO
ORDER BY NRESOLUCION_CODIGO, CUNIDAD_OPERATIVA_CODIGO, CSECUENCIA_FUNCIONAL_CODIGO, CTAREA_CODIGO, VCADENA_GASTO_CODIGO]]>
