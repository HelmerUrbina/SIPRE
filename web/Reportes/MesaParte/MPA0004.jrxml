	<queryString>
		<![CDATA[SELECT CDOCUMENTO_NUMERO||'-'||CPERIODO_CODIGO AS NUMERO, 
INST.VINSTITUCION_DESCRIPCION AS INSTITUCION,
TO_CHAR(DOCU.DUSUARIO_FECCRE,'DD/MM/YYYY HH24:MM:SS') AS RECEPCION
FROM SIPE_DOCUMENTO DOCU INNER JOIN SIPE_INSTITUCION INST ON(
DOCU.CORGANISMO_CODIGO=INST.CORGANISMO_CODIGO AND 
DOCU.CINSTITUCION_CODIGO=INST.CINSTITUCION_CODIGO)
WHERE
CPERIODO_CODIGO=$P{PERIODO} AND
CDOCUMENTO_TIPO=$P{TIPO} AND
CDOCUMENTO_NUMERO=$P{CODIGO}]]>
