	<queryString>
		<![CDATA[SELECT CDOCUMENTO_NUMERO||' - '||CPERIODO_CODIGO AS NUMERO, 
INST.VINSTITUCION_DESCRIPCION AS INSTITUCION,
PK_MESA_PARTES.FUN_NOMBRE_TIPO_DOCUMENTO(NTIPO_DOCUMENTO_CODIGO) AS TIPO_DOCUMENTO,
CDOCUMENTO_NRO_DOCUMENTO AS NRO_DOCUMENTO,
UPPER(VDOCUMENTO_ASUNTO) ASUNTO, UPPER(VDOCUMENTO_OBSERVACION) OBSERVACION,
UPPER(VPOST_FIRMA) AS POST_FIRMA,
TO_CHAR(DOCU.DDOCUMENTO_FECHA_DOCUMENTO,'DD/MM/YYYY') AS FECHA_DOCUMENTO,
TO_CHAR(DOCU.DUSUARIO_FECCRE,'DD/MM/YYYY HH24:MM:SS PM') AS RECEPCION,
NCANTIDAD_LEGAJOS AS LEGAJOS,
NCANTIDAD_FOLIOS AS FOLIOS,
DOCU.VDOCUMENTO_DIGITAL AS DIGITAL
FROM SIPE_DOCUMENTO DOCU INNER JOIN SIPE_INSTITUCION INST ON(
DOCU.CORGANISMO_CODIGO=INST.CORGANISMO_CODIGO AND 
DOCU.CINSTITUCION_CODIGO=INST.CINSTITUCION_CODIGO)
WHERE
CPERIODO_CODIGO=$P{PERIODO} AND
CDOCUMENTO_TIPO=$P{TIPO} AND
CDOCUMENTO_NUMERO=$P{CODIGO}]]>
	</queryString>
