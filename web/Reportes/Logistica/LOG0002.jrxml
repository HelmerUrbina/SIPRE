	</parameter>
	<queryString>
		<![CDATA[SELECT CPERIODO_CODIGO AS PERIODO, 
CUNIDAD_OPERATIVA_CODIGO||'-'||UTIL_NEW.FUN_ABUUOO(CUNIDAD_OPERATIVA_CODIGO) AS UNIDAD_OPERATIVA,
UTIL_NEW.FUN_DEPPTO((SELECT DISTINCT COPPTO FROM TACESO TA WHERE TA.CODPER=CPERIODO_CODIGO AND TA.COUUOO=CUNIDAD_OPERATIVA_CODIGO AND TO_NUMBER(TA.NCSIAF)=TO_NUMBER(NPAAC_CERTIFICADO))) AS FUENTE_FINANCIAMIENTO,
TO_NUMBER(NPAAC_CERTIFICADO) AS CERTIFICADO_SIAF, 
PK_LOGISTICA.FUN_NOMBRE_TIPO_PROCEDIMIENTO(CTIPO_PROCESO_CODIGO)||'-'||VPAAC_NUMERO_PROCESO AS TIPO_PROCESO,
VPAAC_OBJETO AS DESCRIPCION,
NVL(SUM(NPAAC_VALOR_REFERENCIAL),0) AS TOTAL_PROCESO,
CASE WHEN $P{USUARIO} IN ('10714635','43305891','44469132') THEN 'Sección Telemática - OPRE' ELSE 
UTIL_NEW.FUN_DESC_USUARIO($P{USUARIO}) END AS USU_IMPRESION 
FROM SIPE_PAAC WHERE
CPERIODO_CODIGO=$P{PERIODO} AND 
CUNIDAD_OPERATIVA_CODIGO=$P{UNIDAD_OPERATIVA} AND
CPAAC_ESTADO!='AN'
GROUP BY CPERIODO_CODIGO, CUNIDAD_OPERATIVA_CODIGO, NPAAC_CERTIFICADO, CTIPO_PROCESO_CODIGO, VPAAC_NUMERO_PROCESO, VPAAC_OBJETO
ORDER BY CPERIODO_CODIGO, CUNIDAD_OPERATIVA_CODIGO, FUENTE_FINANCIAMIENTO, NPAAC_CERTIFICADO, VPAAC_OBJETO]]>
	</queryString>
