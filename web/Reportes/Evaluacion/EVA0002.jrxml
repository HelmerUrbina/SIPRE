	<queryString>
		<![CDATA[SELECT VANO AS PERIODO,
UTIL.FUN_DEPPTO(TO_NUMBER(DECODE(TRIM(VFUENTE_FINANCIAMIENTO),'00',1,'09',6,'18',5,'19',9,'13',3, TRIM(VFUENTE_FINANCIAMIENTO)))) AS PRESUPUESTO,
UTIL_NEW.FUN_ABUUOO(VCOD_ACT) AS UNIDAD_OPERATIVA,
VNRO_DOCUMENTO_PAGO_M AS RA, DFEC_DOCUMENTO_M AS FECHA,
VREG_SIAF AS SIAF,
COBERTURA, SUM(IMPORTE) AS IMPORTE,
UTIL.FUN_GLOSA_COBERTURA(VANO, TO_NUMBER(DECODE(TRIM(VFUENTE_FINANCIAMIENTO),'00',1,'09',6,'18',5,'19',9,'13',3, TRIM(VFUENTE_FINANCIAMIENTO))), TRIM(COBERTURA)) GLOSA,
CASE WHEN $P{USUARIO} IN ('10714635','43305891','44469132') THEN 'Sección Telemática - OPRE' ELSE 
UTIL_NEW.FUN_DESC_USUARIO($P{USUARIO}) END AS USU_IMPRESION
FROM V_RESOL_ADMINISTRATIVAS@DBLINK_ECON  WHERE 
VANO=$P{PERIODO} AND 
TO_NUMBER(DECODE(TRIM(VFUENTE_FINANCIAMIENTO),'00',1,'09',6,'18',5,'19',9,'13',3, TRIM(VFUENTE_FINANCIAMIENTO)))=$P{PRESUPUESTO} AND 
VCOD_ACT=$P{UNIDAD_OPERATIVA}
GROUP BY VANO, VFUENTE_FINANCIAMIENTO, VCOD_ACT, DFEC_DOCUMENTO_M, VNRO_DOCUMENTO_PAGO_M, DFEC_DOCUMENTO_M, VREG_SIAF, COBERTURA
ORDER BY FECHA, VNRO_DOCUMENTO_PAGO_M]]>
	</queryString>
